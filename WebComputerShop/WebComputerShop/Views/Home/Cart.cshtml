@model IEnumerable<WebComputerShop.Models.Product>

@foreach (var item in Model)
{
    <div>
        <img src="~/Home/GetProductImage/@item.Id" />
        <h4>Name:@item.Name</h4>
        <h4>ImgUrl:@item.ImgUrl</h4>
        <h4>Item_Discription:@item.Item_Discription</h4>
        @Html.ActionLink("Remove", "RemoveFromCart", "Home", new { id = item.Id },new {@class ="btn btn-danger" })
    </div>
}